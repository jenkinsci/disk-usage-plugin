<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:section title="${%Disk usage}">
      <f:entry title="">
          <f:checkbox name="disk_usage.showGraph" checked="${descriptor.showGraph}" />
          ${%Show disk usage trend graph on the project page}
      </f:entry>

      <f:entry title="">
          ${%Overall graph history length (days)}
	      <f:textbox name="disk_usage.historyLength" value="${descriptor.historyLength}" />
      </f:entry>
  </f:section>
</j:jelly>